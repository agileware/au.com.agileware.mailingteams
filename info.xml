<?xml version="1.0"?>
<extension key="au.com.agileware.mailingteams" type="module">
  <file>mailingteams</file>
  <name>MailingTeams</name>
  <description>Plugin for CiviTeams extension that provides support for Team-based access control to CiviMail</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Agileware</author>
    <email>support@agileware.com.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/agileware/au.com.agileware.mailingteams</url>
    <url desc="Documentation">https://github.com/agileware/au.com.agileware.mailingteams/wiki</url>
    <url desc="Support">https://github.com/agileware/au.com.agileware/mailingteams/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-11-08</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.67</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/MailingTeam</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_MailingTeam_Upgrader</upgrader>
</extension>
